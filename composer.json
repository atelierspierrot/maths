{
    "name": "atelierspierrot/maths",
    "type": "library",
    "description": "Some PHP classes to do mathematics",
    "keywords": [ "library", "helpers", "mathematics" ],
    "homepage": "http://github.com/atelierspierrot/maths",
    "license": "GPL-3.0",
    "authors": [{
        "name": "Les Ateliers Pierrot & contributors",
        "email": "contact@ateliers-pierrot.fr",
        "homepage": "http://www.ateliers-pierrot.fr/"
    },{
        "name": "Pierre Cassat",
        "homepage": "http://e-piwi.fr/"
    }],
    "require": {
        "php": ">=5.3.0",
        "atelierspierrot/patterns": "1.*",
        "atelierspierrot/library": "1.*"
    },
    "require-dev": {
        "sami/sami": "@stable",
        "fabpot/php-cs-fixer": "@stable",
        "phpmd/phpmd" : "@stable"
    },
    "autoload": { "psr-4": {  "Maths\\": "src/Maths/" } },
    "minimum-stability": "dev",
    "scripts": {
        "build-doc": "php vendor/bin/sami.php update .sami.php",
        "update-doc": "php vendor/bin/sami.php update .sami.php",
        "cs-fixer": "php vendor/bin/php-cs-fixer fix -v"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
